{
  "name": "browsersync",
  "version": "1.0.0",
  "description": "browsersync",
  "main": "index.js",
  "babel": {
    "presets": [
      [
        "latest"
      ],
      [
        "react"
      ]
    ]
  },
  "scripts": {
    "build": "rm -rf dist && browser-sync start --server --files **/*.css, **/*.html,**/*.js",
    "start": "webpack --watch",
    "test": "rm -rf dist && webpack --config webpack.dev.config.js --progress --display-modules --colors --display-reasons",
    "webpack": "rm -rf dist && webpack --config webpack.config.js --progress --display-modules --colors --display-reasons",
    "clean": "rm -rf dist"
  },
  "repository": "https://github.com/xuezhenguo/React-Browsersync-webpack.git",
  "author": "xuezhenguo <xuezhenguo@didichuxing.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "postcss-loader": "^2.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1"
  }
}
